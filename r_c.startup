ip a a 172.18.163.254/23 dev eth0
ip a a 172.18.167.130/25 dev eth1
ip r a default via 172.18.167.129

echo "nameserver 9.9.9.9" > /etc/resolv.conf

apt update
apt install -y isc-dhcp-server
echo "
DHCPDv4_CONF=/etc/dhcp/dhcpd.conf
DHCPDv4_PID=/var/run/dhcpd.pid
INTERFACESv4="eth0"
" > /etc/default/isc-dhcp-server
echo "
ddns-update-style none;

subnet 172.18.162.0 netmask 255.255.254.0 {
	range 172.18.162.1 172.18.162.176;
	default-lease-time 21600;
	max-lease-time 43200;
	option routers 172.18.163.254;
	option domain-name-servers 9.9.9.9;
}
" > /etc/dhcp/dhcpd.conf
systemctl start isc-dhcp-server